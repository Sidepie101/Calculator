        <Button x:Name="HelpButton" Content="?" Grid.Row="2" Grid.Column="2" HorizontalAlignment="Stretch" Height="NaN" VerticalAlignment="Stretch" Width="NaN" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
        <Button x:Name="InverseButton" Content="1/X" Grid.Row="3" Height="NaN" Width="NaN" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="ModuloButton" Content="%" Grid.Row="2" Height="NaN" Width="NaN" Grid.Column="1" Margin="0,63,0,0" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="FactorielButton" Content="n!" Grid.Row="2" Height="NaN" Width="NaN" Grid.Column="2" Margin="0,63,0,0" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
