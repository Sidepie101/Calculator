        <Button x:Name="EightButton" Content="8" Grid.Row="3" Height="NaN" Width="NaN" Grid.Column="1" Margin="0,58,0,58" Grid.RowSpan="3" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="NineButton" Content="9" Grid.Row="3" Height="NaN" Width="NaN" Grid.Column="2" Margin="0,58,0,58" Grid.RowSpan="3" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="MultiplicationButton" Content="X" Grid.Row="3" Height="NaN" Width="NaN" Grid.Column="3" Margin="0,58,0,58" Grid.RowSpan="3" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
        <Button x:Name="FourButton" Content="4" Grid.Row="5" Height="NaN" Width="NaN" Margin="0,0,0,57" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="FiveButton" Content="5" Grid.Row="5" Height="NaN" Width="NaN" Grid.Column="1" Margin="0,0,0,57" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="SixButton" Content="6" Grid.Row="5" Height="NaN" Width="NaN" Grid.Column="2" Margin="0,0,0,57" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="MoinsButton" Content="-" Grid.Row="5" Height="NaN" Width="NaN" Grid.Column="3" Margin="0,0,0,57" Grid.RowSpan="2" Click="MoinsButton_Click" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
        <Button x:Name="OneButton" Content="1" Grid.Row="5" Height="NaN" Width="NaN" Margin="0,57,0,0" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="TwoButton" Content="2" Grid.Row="5" Height="NaN" Width="NaN" Grid.Column="1" Margin="0,57,0,0" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="ThreeButton" Content="3" Grid.Row="5" Height="NaN" Width="NaN" Grid.Column="2" Margin="0,57,0,0" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="AdditionButton" Content="+" Grid.Row="5" Height="NaN" Width="NaN" Grid.Column="3" Margin="0,57,0,0" Grid.RowSpan="2" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
        <Button x:Name="ZeroButton" Content="0" Grid.Row="7" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="PointButton" Content="." Grid.Row="7" Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="{DynamicResource {x:Static SystemColors.InactiveBorderBrushKey}}"/>
        <Button x:Name="EqualButton" Grid.Column="2" Content="=" Grid.ColumnSpan="2" Grid.Row="7" Background="#FF7B9BF6"/>

    </Grid>
</Window>
